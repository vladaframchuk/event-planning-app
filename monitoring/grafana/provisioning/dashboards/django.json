{
  "id": null,
  "uid": "django-overview",
  "title": "Django and Celery",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "type": "timeseries",
      "title": "HTTP Request Rate",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum(rate(django_http_requests_total_by_method_total[5m]))",
          "legendFormat": "req/s"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "req/s"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "HTTP P95 Latency",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(django_http_requests_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Database Query Time",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "rate(django_db_query_duration_seconds_sum[5m]) / rate(django_db_query_duration_seconds_count[5m])",
          "legendFormat": "avg query time"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Celery Queue Latency P95",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(celery_task_queue_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "queue p95"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Active Celery Tasks",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum(celery_tasks_active)",
          "legendFormat": "active"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "tasks"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "WebSocket Connections",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "channels_ws_active_connections",
          "legendFormat": "{{consumer}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "connections"
        },
        "overrides": []
      }
    }
  ],
  "time": {
    "from": "now-6h",
    "to": "now"
  }
}
